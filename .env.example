# API Configuration
API_URL=https://api.example.com/graphql

# Google OAuth Configuration
GOOGLE_WEB_CLIENT_ID=
GOOGLE_IOS_CLIENT_ID=
GOOGLE_ANDROID_CLIENT_ID=
GOOGLE_SERVER_CLIENT_ID=

# Application Version
APP_VERSION=0.2.1

# Sentry Configuration
# Required: DSN for your Sentry project
SENTRY_DSN=

# Optional: Environment name (development, staging, production)
# If not set, defaults to MODE or NODE_ENV
SENTRY_ENVIRONMENT=development

# Optional: Release version string
# If not set, automatically generated as "guest-spot@<package.json.version>"
SENTRY_RELEASE=

# Optional: Sample rate for performance monitoring (0.0 to 1.0)
# Example: 0.1 means 10% of requests will be traced
SENTRY_TRACES_SAMPLE_RATE=0.1

# Optional: Enable Sentry debug logging (true/false)
# WARNING: Only enable locally, not in production
SENTRY_DEBUG=false

# Sentry Source Maps Upload (for production builds only)
# Required for automatic source maps upload during build
# Keep these values secure - do NOT commit to repository
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=
