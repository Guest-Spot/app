{
  "name": "guest-spot",
  "version": "0.0.3",
  "description": "A Quasar Project",
  "productName": "GuestSpot",
  "author": "denisgrushkin <denis.grushkin@maddevs.io>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build -m spa",
    "postinstall": "quasar prepare",
    "mobile:add": "quasar mode add cordova",
    "mobile:remove": "quasar mode remove cordova",
    "mobile:dev": "quasar dev -m cordova",
    "mobile:build": "quasar build -m cordova",
    "mobile:build:android": "quasar build -m cordova -T android",
    "mobile:build:ios": "quasar build -m cordova -T ios",
    "mobile:run:android": "quasar dev -m cordova -T android",
    "mobile:run:ios": "quasar dev -m cordova -T ios",
    "mobile:emulate:android": "quasar dev -m cordova -T android --emulator",
    "mobile:emulate:ios": "quasar dev -m cordova -T ios --emulator",
    "mobile:clean": "quasar clean",
    "mobile:prepare": "quasar prepare",
    "capacitor:add:ios": "quasar mode add capacitor -T ios",
    "capacitor:add:android": "quasar mode add capacitor -T android",
    "capacitor:remove:ios": "quasar mode remove capacitor -T ios",
    "capacitor:remove:android": "quasar mode remove capacitor -T android",
    "capacitor:dev": "quasar dev -m capacitor",
    "capacitor:build": "quasar build -m capacitor",
    "capacitor:build:ios": "quasar build -m capacitor -T ios",
    "capacitor:build:android": "quasar build -m capacitor -T android",
    "capacitor:sync:ios": "quasar dev -m capacitor --sync -T ios",
    "capacitor:sync:android": "quasar dev -m capacitor --sync -T android",
    "capacitor:run:android": "quasar dev -m capacitor -T android",
    "capacitor:run:ios": "quasar dev -m capacitor -T ios",
    "capacitor:open:android": "quasar dev -m capacitor -T android --open",
    "capacitor:open:ios": "quasar dev -m capacitor -T ios --open"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@quasar/extras": "^1.16.4",
    "@supabase/supabase-js": "^2.56.1",
    "axios": "^1.2.1",
    "browser-image-compression": "^2.0.2",
    "pinia": "^3.0.1",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.12",
    "vue3-carousel": "^0.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@quasar/cli": "^2.0.0",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "autoprefixer": "^10.4.2",
    "dotenv": "^17.2.1",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.9.0",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
